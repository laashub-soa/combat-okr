.okrDetail-container{
  display: flex;
  flex-direction:column;
}
.okrshow{
  padding: 10px;
}
.objective-list{
  margin-bottom: 20px;
  background-color:#f5f5f5;
}
.objective-tilte{
  line-height: 30px;
  font-size: 18px;
  color: #333;
}
.objective-list .create-time{
  position: relative;
  padding-left:15px;
  line-height: 30px;
  font-size: 16px;
  color:#999;

}
.create-time:before{
  content: '';
  position: absolute;
  top:10px;
  left:5px;
  display: inline-block;
  width: 4px;
  height: 4px;
  border-radius: 100%;
  background-color: #999;
}
.keyresult-list{
  padding-left: 10px;
  position: relative;
}
.keyresult-list:before{
  content: '';
  position: absolute;
  top:0px;
  left:4px;
  display: inline-block;
  width: 1px;
  height:100%;
  background-color: #f5f5f5;
}
.keyresult-item{
  margin-bottom: 15px;
  padding-left: 10px;
}
.keyresult-content{
  display: flex;
  justify-content: space-between;
}
.keyresult-title{
  font-size: 16px;
  color:#333;
  line-height: 30px;
}
.keyresult-title:before{
  content: '';
  position: absolute;
  top:10px;
  left:5px;
  display: inline-block;
  width:20px;
  height:20px;
  border-radius: 100%;
  background-color: #999;
}
.keyresult-completed{
  font-size: 16px;
  color:#999;
  line-height: 30px;
}
.todo-item{
  line-height: 30px;
  color:#999;
  text-decoration: line-through;
}
.todo-item:before{
  content: '';
  position: absolute;
  top:10px;
  left:5px;
  display: inline-block;
  width: 4px;
  height: 4px;
  border-radius: 100%;
  background-color: #999;
}


